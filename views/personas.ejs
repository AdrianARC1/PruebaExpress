<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <%- include('partials/navegacion.ejs') %> 

    <% resultado_ejemplo.forEach(function (item) { %> 
        <div>
            <h1>Edad: <%= item.DNI %></h1>   
            <h1>Nombre: <%= item.NOMBRE %></h1>   
            <h1>Apellido: <%= item.APELLIDO %></h1>
            <h1>Edad: <%= item.EDAD %></h1>
            <a href="/delete/<%= item.id %>">Eliminar persona</a>
            <a href="/update/<%= item.id %>">Editar persona</a>
        </div>
    <%  }); %>
</body>
</html>